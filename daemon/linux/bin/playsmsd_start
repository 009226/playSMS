#!/bin/bash

. /etc/default/playsms

CWD=`pwd`
cd $PLAYSMS_BIN
./playsmsd $PLAYSMS_PATH >/dev/null 2>&1 &
cd $CWD

CWD=`pwd`
cd $PLAYSMS_BIN
./recvsmsd $PLAYSMS_PATH >/dev/null 2>&1 &
cd $CWD

CWD=`pwd`
cd $PLAYSMS_BIN
./sendsmsd $PLAYSMS_PATH >/dev/null 2>&1 &
cd $CWD

CWD=`pwd`
cd $PLAYSMS_BIN
./dlrssmsd $PLAYSMS_PATH >/dev/null 2>&1 &
cd $CWD

exit 0
